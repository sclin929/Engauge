<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.engauge"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="20" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.app.engauge.FirebaseApp"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.app.engauge.LoginActivity"
            android:label="@string/login_activity_label" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.app.engauge.SignUpActivity"
            android:label="@string/signup_activity_label" >
        </activity>
        <activity
            android:name="com.app.engauge.HomepageActivity"
            android:label="@string/homepage_activity_label" >
        </activity>
        <activity
            android:name="com.app.engauge.ViewGroupActivity"
            android:label="@string/view_group_activity_label" >
        </activity>
        <activity
            android:name="com.app.engauge.AddQuestionActivity"
            android:label="@string/add_question_activity_label" >
        </activity>
    </application>

</manifest>